    #createUserForm {
      background-color: rgba(26, 24, 20, 0.9);
      border-radius: 0;
    }

    #submit {
      color: #ffffff;
      padding: 10px 20px;
      border: 2px solid #c7a668;
      border-radius: 5px;
      cursor: pointer;
      background-color: rgba(199, 166, 104, 0);
      transition: background-color 0.3s;
    }

    #submit:hover {
      background-color: rgba(199, 166, 104);
    }
  </style>
